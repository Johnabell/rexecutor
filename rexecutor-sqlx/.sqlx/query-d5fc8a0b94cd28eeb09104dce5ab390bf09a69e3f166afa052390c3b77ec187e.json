  "query": "UPDATE rexecutor_jobs\n            SET status = 'executing', attempted_at = timezone('UTC'::text, now())\n            WHERE id = $1 AND status != 'executing'\n            RETURNING\n                id,\n                status AS \"status: JobStatus\",\n                executor,\n                data,\n                attempt,\n                max_attempts,\n                errors,\n                inserted_at,\n                scheduled_at,\n                attempted_at,\n                completed_at,\n                cancelled_at,\n                discarded_at\n            ",
