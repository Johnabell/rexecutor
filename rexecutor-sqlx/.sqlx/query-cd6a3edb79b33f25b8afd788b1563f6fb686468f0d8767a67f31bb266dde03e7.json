{
  "db_name": "PostgreSQL",
  "query": "UPDATE rexecutor_jobs\n            SET\n                status = 'executing',\n                attempted_at = timezone('UTC'::text, now()),\n                attempt = attempt + 1\n            WHERE id IN (\n                SELECT id from rexecutor_jobs\n                WHERE scheduled_at - timezone('UTC'::text, now()) < '00:00:00.1'\n                AND status in ('scheduled', 'retryable')\n                AND executor = $1\n                LIMIT 1\n                FOR UPDATE SKIP LOCKED\n            )\n            RETURNING\n                id,\n                status AS \"status: JobStatus\",\n                executor,\n                data,\n                attempt,\n                max_attempts,\n                errors,\n                inserted_at,\n                scheduled_at,\n                attempted_at,\n                completed_at,\n                cancelled_at,\n                discarded_at\n            ",
